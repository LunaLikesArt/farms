<br />
<% @farms.each do |farm| %>
<div class="row">
  <div class="booyah-box col-xs-10 col-xs-offset-1">
    <div class="row">

      <div class="col-sm-4">
        <%= image_tag 'tree.jpeg', class: 'img-responsive' %>
      </div>

      <div class="col-sm-8">
        <br />
        <h3><%= link_to farm.name, farm_path(farm) %></h3>
        <p />
        <%= farm.address %>
        <p />
        <%= farm.description %>
      </div>
      
    </div>
  </div>
</div>
<br />
<% end %>
