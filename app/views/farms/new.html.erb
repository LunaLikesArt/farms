<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h2>Enter Your Farm Details</h2>
  <br />
  <%= simple_form_for @farm do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.submit 'Save', class: 'btn btn-sm btn-success pull-right' %>


  <% @farm.photos.each do |photo| %>
    <%= image_tag photo.picture %>
    <%= photo.caption %>
  <% end %>
  <% end %>
